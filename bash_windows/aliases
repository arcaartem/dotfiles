alias ll='ls -l --color'
alias llh='ls -al --color'
alias ld='ll | grep ^d'
alias lf='ll | grep ^-'
alias ldh='llh | grep ^d'
alias lfh='llh | grep ^-'
alias ..='cd ..'
alias re='source ~/.bash_profile'
alias rs='source ~/.bash_ssh-agent'
alias vs='vim ~/.bash_ssh-agent'
alias ea='vim ~/dotfiles/bash/aliases && re'
alias cls='clear'
alias vim='vim -N'
alias vi='vim -N'
alias e='exit'

function take {
mkdir -p $1 
cd $1
}
